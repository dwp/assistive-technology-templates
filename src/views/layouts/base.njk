<!doctype html>
<html lang="en">
  <head>

    <meta charset="utf-8">
    <title>{{details.assistiveTech.name}} test from {{details.date}} - {{details.site.name}}</title>
    <meta name="description" content="{{template.title}}">
    <style>
      {# Put CSS in page so it works when downloaded #}
      {% include 'styles/styles.css' %}
    </style>
  </head>

  <body id="top">
    {% include 'components/skip-link/template.njk' %}
    <div class="at-container">
      <header role="banner">
        <h1 id="{{details.assistiveTech.id}}-testing">
          {{details.assistiveTech.name}} Testing
        </h1>
        {% block headerContent %}{% endblock %}
      </header>

      {% block nav %}{% endblock %}

      <main id="main" role="main">
        {% block content %}{% endblock %}
        {% include 'components/back-to-top/template.njk' %}
      </main>
      
      <footer role="contentinfo">
        {% block footer %}{% endblock %}
      </footer>
    </div>
  </body>
</html>
